
public class Main {
    static int duplicate(int arr[]) {
    	int size =0;
    	
    	for(int i =0 ;i< arr.length;i++) {
    		
    		if(arr[i]!=arr[i+1]) {
    			arr[size++] = arr[i];
    		}
    	}
    	
    	return size;
    }
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		

	}

}
